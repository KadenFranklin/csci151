<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="demo.DemoController">
   <center>
      <Pane fx:id="pane" onMousePressed="#initialize" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button layoutX="59.0" layoutY="2.0" mnemonicParsing="false" onAction="#stop" text="Stop" />
            <Button layoutX="100.0" layoutY="2.0" mnemonicParsing="false" onAction="#start" text="Start" />
         </children></Pane>
   </center>
</BorderPane>
